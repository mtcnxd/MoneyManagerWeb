{
    "name": "capsule/di",
    "description": "A PSR-11 compliant autowiring dependency injection container.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul M. Jones",
            "email": "pmjones@pmjones.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/container": "~1.0"
    },
    "provide": {
        "psr/container-implementation" : "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Capsule\\Di\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Capsule\\Di\\": "tests/"
        }
    }
}
